@{
    Layout = null;
    ViewBag.GlobalTitle = AdminLTE1.App_Start.GlobalVariables.WebsiteName;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>
        @if (!string.IsNullOrWhiteSpace(ViewBag.Title))
        {
            @ViewBag.Title@: -
        }
        @ViewBag.GlobalTitle
    </title>
    <link href="~/Content/img/logo.png" rel="shortcut icon">
    <link href="~/Content/bootstrap.min.css" rel="stylesheet">
    <link href="~/Fonts/font-awesome/css/font-awesome.css" rel="stylesheet">
    <link href="~/Content/animate.css" rel="stylesheet">
    <link href="~/Content/style.css" rel="stylesheet">
</head>
<body class="gray-bg">
    <div class="middle-box text-center loginscreen  animated fadeInDown">
        <div>
            <div>
                <h1 class="logo-name">404</h1>
            </div>
            <h3>Page Not Found</h3>
            <p class="m-t alert alert-danger">
                <small>
                    Page That You Requested Is Not Found
                    </br>
                    Please Contact Your Web Administrator
                    </br>
                    <a href="@Url.Action("Index","Home")">Go Home</a>
                </small>
            </p>
        </div>
    </div>
</body>
</html>
